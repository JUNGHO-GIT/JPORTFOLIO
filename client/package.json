{
  "name": "JREACT-client",
  "homepage": "/JREACT",
  "version": "1.0.0",
  "main": "index.tsx",
  "type": "module",
  "license": "Apache-2.0",
  "private": true,
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off",
      "react-hooks/rules-of-hooks": "warn",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "npm-clean": "npm cache clean --force",
    "npm-reset": "powershell -Command \"Remove-Item -Recurse -Force package-lock.json, node_modules; npm install --legacy-peer-deps\"",
    "gcloud": "node gcloud.cjs"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.7",
    "@react-hooks-library/core": "^0.6.2",
    "@testing-library/jest-dom": "^6.6.3",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "axios": "^1.7.7",
    "material-ui-popup-state": "^5.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "react-scripts": "^5.0.1",
    "swiper": "^11.1.15",
    "swiper-react": "^1.0.0",
    "typed.js": "^2.1.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@craco/craco": "^7.1.0",
    "@swc/core": "^1.9.2",
    "@testing-library/react": "^16.0.1",
    "@types/axios": "^0.14.4",
    "@types/history": "^5.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "compression-webpack-plugin": "^11.1.0",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "react-app-alias": "^2.2.2",
    "swc-minify-webpack-plugin": "^2.1.3",
    "thread-loader": "^4.0.4",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "webpack": "^5.96.1"
  }
}
